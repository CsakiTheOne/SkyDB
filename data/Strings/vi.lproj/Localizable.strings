"ap14_story_tree_1a2a3b4b_cont_abv" = "Mang trong mình niềm tự hào...";
"ap14_story_tree_1a2a3b_abv" = "Ánh sáng của nó bao trùm...";
"ap14_story_tree_1a2a3b_cont" = "Cuối cùng khi nó nhìn xuống bên dưới...";
"ap14_story_tree_1a2a_cont" = "Cho đến khi...";
"ap14_story_tree_1a2b" = "Nó trở nên mờ dần";
"ap14_story_tree_1a2b3a" = "Ánh sáng của nó gần như lụi tàn trên bầu trời";
"ap14_story_tree_1a2b3a4a" = "Nó tách thành vô số luồng sáng, nhẹ nhàng đáp xuống mặt đất bên dưới.";
"ap14_story_tree_1a2b3a4a_abv" = "Nó tách thành...";
