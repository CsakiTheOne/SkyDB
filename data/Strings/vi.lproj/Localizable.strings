"ap14_story_tree_1a2a3a4b" = "Nó ban ánh sáng cho một đứa trẻ cô đơn lang thang trên Hoang Mạc";
"ap14_story_tree_1a2a3a4b_abv" = "Nó ban ánh sáng...";
"ap14_story_tree_1a2a3a4b_cont" = "Được dẫn đường bởi ánh sáng, đứa trẻ đã nhìn thấy con đường phía trước. Kể từ đó, nó sẽ không bao giờ đơn độc.";
"ap14_story_tree_1a2a3a4b_cont_abv" = "Được dẫn đường bởi ánh sáng...";
"ap14_story_tree_1a2a3a_abv" = "Nó không còn chứa được...";
"ap14_story_tree_1a2a3a_cont" = "Với hành động quên mình...";
"ap14_story_tree_1a2a3b" = "Ánh sáng của nó bao trùm cả bầu trời";
"ap14_story_tree_1a2a3b4a" = "Ngôi sao không nhìn thấy gì ngoài ánh sáng chói lòa của chính mình";
"ap14_story_tree_1a2a3b4a_abv" = "Ngôi sao không nhìn thấy gì...";
"ap14_story_tree_1a2a3b4a_cont" = "Lặng lẽ và đơn độc, nó tắt dần ánh sáng cho đến khi những hình bóng quay trở lại. Nó vô tư tỏa sáng mà không cần mục đích.";
"ap14_story_tree_1a2a3b4a_cont_abv" = "Lặng lẽ và đơn độc...";
"ap14_story_tree_1a2a3b4b" = "Nó nhìn thấy một đứa trẻ đang tự tin rảo bước trên Hoang Mạc đầy ánh sáng";
"ap14_story_tree_1a2a3b4b_abv" = "Nó nhìn thấy một đứa trẻ...";
