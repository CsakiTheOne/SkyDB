"ap11_fetch_03_belltower_02" = "È più grande di tutto il mio pianeta.";
"ap11_fetch_03_belltower_03" = "Il vento la fa suonare.";
"ap11_fetch_03_belltower_04" = "Guarda, è decorata di campane.";
"ap11_fetch_03_belltower_05" = "Proprio come il cielo è decorato di stelle.";
"ap11_fetch_03_belltower_06" = "Le campane devono essere per le creature.";
"ap11_fetch_03_belltower_07" = "Tutti i bambini lo sanno.";
"ap11_fetch_03_belltower_08" = "Raggiungiamo quella statua.";
"ap11_fetch_03_bye_01" = "Per caso, sai...";
"ap11_fetch_03_bye_02" = "che cosa vuol dire \"addomesticare\"?";
"ap11_fetch_03_bye_03" = "Una volpe mi ha detto che significa creare dei legami.";
"ap11_fetch_03_bye_04" = "Guarda!";
"ap11_fetch_03_bye_05" = "Se creiamo dei legami, avremo bisogno l'uno dell'altro.";
"ap11_fetch_03_bye_06" = "Tra tutti i milioni di ragazzini...";
"ap11_fetch_03_bye_07" = "Tu sarai per me unico al mondo.";
"ap11_fetch_03_bye_08" = "E io sarò per te unico al mondo.";
"ap11_fetch_03_end_quote" = "Si arrischia di piangere un poco se ci si è lasciati addomesticare.";
"ap11_fetch_03_geyser_01" = "Questo è un vulcano!";
"ap11_fetch_03_geyser_02" = "Ho tre vulcani, che spazzo ogni settimana.";
"ap11_fetch_03_geyser_03" = "Due in attività, uno spento.";
"ap11_fetch_03_geyser_04" = "Perché non si sa mai.";
"ap11_fetch_03_geyser_06" = "Non è il suono di una cascata?";
"ap11_fetch_03_geyser_07" = "Cerchiamole!";
"ap11_fetch_03_outro_01" = "Ero pronta ad affrontare le tigri,";
"ap11_fetch_03_outro_02" = "con le mie fantastiche spine.";
"ap11_fetch_03_outro_03" = "Ma il mio amico perse interesse,";
"ap11_fetch_03_outro_04" = "mentre vacillavo nel vento.";
"ap11_fetch_03_outro_05" = "Si guarderà indietro e si ricorderà di me?";
"ap11_fetch_03_statue_01" = "Cosa sono queste creature?";
"ap11_fetch_03_statue_02" = "Si possono addomesticare?";
"ap11_fetch_03_statue_03" = "Come un amico per la vita?";
"ap11_fetch_03_statue_05" = "Cos'è quel rumore?";
"ap11_fetch_03_statue_06" = "Vieni con me!";
"ap11_fetch_03_tunnel_01" = "Continuiamo a esplorare!";
"ap11_fetch_03_tunnel_02" = "Dove conduce questo?";
"ap11_fetch_03_tunnel_03" = "Wow!!!";
"ap11_fetch_03_tunnel_04" = "È stato divertentissimo!";
"ap11_fetch_03_waterfall_01" = "Lo senti?";
"ap11_fetch_03_waterfall_02" = "La cascata sta cantando...";
"ap11_fetch_03_waterfall_03" = "L'acqua può essere buona anche per il cuore...";
"ap11_fetch_03_waterfall_hint_01" = "Da dove proviene l'acqua?";
"ap11_fetch_03_waterfall_hint_02" = "Andiamo a cercare la fonte.";
"ap11_fetch_04_burn01_01" = "Ci sono semi buoni e semi cattivi.";
"ap11_fetch_04_burn02_01" = "I semi cattivi, come i baobab, possono diventare fastidiosi se non te ne occupi subito.";
"ap11_fetch_04_burn03_01" = "Se si tratta di un ramoscello inoffensivo o di una rosa, uno lo farebbe crescere dove vuole.";
"ap11_fetch_04_burn04_01" = "Questi fiori mi ricordano la mia rosa lontana.";
"ap11_fetch_04_burn05_01" = "I fiori sono delicati.";
"ap11_fetch_04_burn05_02" = "A volte è necessario ripararli dal vento.";
"ap11_fetch_04_burn06_01" = "Ascoltavo la mia rosa quando brontolava o quando si vantava...";
"ap11_fetch_04_burn06_02" = "e a volte anche quando non diceva niente.";
"ap11_fetch_04_burn07_01" = "È il tempo che tu hai perduto per la tua rosa...";
"ap11_fetch_04_burn07_02" = "che ha fatto la tua rosa così importante.";
"ap11_fetch_04_burn_start_hint" = "Rimuovi l'oscurità per occuparti dei fiori.";
"ap11_fetch_04_bye01_01" = "Queste rose sono bellissime.";
"ap11_fetch_04_bye01_02" = "Ma sono vuote.";
"ap11_fetch_04_bye01_03" = "La mia rosa è unica al mondo.";
"ap11_fetch_04_bye01_04" = "La mia rosa profumava tutto il mio pianeta...";
"ap11_fetch_04_bye01_05" = "Ma non sapevo rallegrarmene.";
"ap11_fetch_04_bye02_01" = "La mia volpe ha detto...";
"ap11_fetch_04_bye02_02" = "Non si vede bene che col cuore.";
"ap11_fetch_04_bye02_03" = "L'essenziale è invisibile agli occhi.";
"ap11_fetch_04_end_quote" = "Le stelle sono belle per un fiore che non si vede...";
"ap11_fetch_04_meet_01" = "C'è qualcosa che freme laggiù.";
"ap11_fetch_04_meet_02" = "Questi fiori stanno soffocando!";
"ap11_fetch_04_outro_01" = "La sua risata divenne silenzio.";
"ap11_fetch_04_outro_02" = "La mia vanità divenne una sconfitta.";
"ap11_fetch_04_outro_03" = "Senza il mio amico al mio fianco,";
"ap11_fetch_04_outro_04" = "questi tramonti sono meno dolci.";
"ap11_fetch_04_outro_05" = "Mi chiedo se un giorno proverà rimorso.";
"ap11_fetch_05_bye_01" = "Grazie...";
"ap11_fetch_05_bye_02" = "Sono felice di avere la tua amicizia.";
"ap11_fetch_05_end_quote" = "Tu diventi responsabile per sempre di quello che hai addomesticato.";
"ap11_fetch_05_meet_01" = "Ho trovato questa nella grotta che abbiamo visitato.";
"ap11_fetch_05_meet_02" = "Come funziona?";
"ap11_fetch_05_meet_03" = "Proveresti tu al posto mio?";
"ap11_fetch_05_outro_01" = "(colpo di tosse)";
"ap11_fetch_05_outro_02" = "Di giorno profumavo il suo pianeta.";
"ap11_fetch_05_outro_03" = "Di notte deperivo.";
"ap11_fetch_05_outro_04" = "Ma lui non vedeva la mia amicizia";
"ap11_fetch_05_outro_05" = "né la gioia nella mia anima.";
"ap11_fetch_05_outro_06" = "Se ne renderà mai conto...?";
"ap11_fetch_05_reflect_01" = "Avrei dovuto giudicarlo dagli atti...";
"ap11_fetch_05_reflect_02" = "non dalle parole.";
"ap11_fetch_05_reflect_04" = "Credi che io sia responsabile della mia rosa?";
"ap11_fetch_05_sunset_01" = "Mi piacciono tanto i tramonti.";
"ap11_fetch_05_sunset_02" = "Proprio come a casa.";
"ap11_fetch_05_sunset_03" = "Un giorno ho visto il sole tramontare quarantatré volte!";
"ap11_fetch_05_sunset_04" = "Chissà se la mia rosa osserva ancora il tramonto? ";
"ap11_fetch_05_sunset_05" = "Chissà se è al riparo dal vento?";
"ap11_fetch_05_sunset_07" = "Puoi suonare dell'altra musica?";
"ap11_fetch_06_campfire_01" = "E poi ti voglio fare un regalo...";
"ap11_fetch_06_campfire_02" = "E sarà proprio questo il mio regalo...";
"ap11_fetch_06_campfire_03" = "Anch'io, oggi, ritorno a casa...";
"ap11_fetch_06_campfire_04" = "Quando tu guarderai il cielo, la notte,";
"ap11_fetch_06_campfire_05" = "visto che io abiterò in una di esse,";
"ap11_fetch_06_campfire_06" = "visto che io riderò in una di esse,";
"ap11_fetch_06_campfire_07" = "allora sarà per te come se tutte le stelle ridessero.";
"ap11_fetch_06_campfire_08" = "È molto più lontano... è molto più difficile...";
"ap11_fetch_06_campfire_09" = "Avrò ben più paura questa sera.";
"ap11_fetch_06_campfire_10" = "Sembrerà che io mi senta male.";
"ap11_fetch_06_campfire_11" = "Non venire a vedere.";
"ap11_fetch_06_campfire_12" = "Guarderai le stelle, la notte.";
"ap11_fetch_06_campfire_13" = "La mia stella sarà per te una delle stelle.";
"ap11_fetch_06_campfire_14" = "Allora, tutte le stelle, ti piacerà guardarle...";
"ap11_fetch_06_campfire_15" = "Tutte, saranno tue amiche.";
"ap11_fetch_06_end_quote" = "Non sapevo bene come toccarlo, come raggiungerlo. Il paese delle lacrime è così misterioso.";
"ap11_fetch_06_outro_01" = "(colpo di tosse)";
"ap11_fetch_06_outro_02" = "La mia ora è arrivata...";
"ap11_fetch_06_outro_03" = "Ho chiesto perdono.";
"ap11_fetch_06_outro_04" = "Ma lui non è tornato.";
"ap11_fetch_06_outro_05" = "Tornerà mai a cercarmi?";
"ap11_fetch_07_end_quote" = "La mia stella sarà per te una delle stelle. Allora, tutte le stelle, ti piacerà guardarle... Tutte, saranno tue amiche.";
"ap11_fetch_07_end_quote_01" = "La mia stella sarà per te una delle stelle.";
"ap11_fetch_07_end_quote_02" = "Allora, tutte le stelle, ti piacerà guardarle.";
