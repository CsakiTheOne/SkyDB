"ap11_spirit_beckon_01" = "Kau tahu mereka tak bisa bergerak, kan? ";
"ap11_spirit_beckon_02" = "Aku berhak memerintah mereka! Perintahku masuk akal.";
"ap11_spirit_beckon_03" = "Mulai dari sekarang, diam di tempatmu berada. ";
"ap11_spirit_beckon_04" = "Orang dewasa aneh sekali. ";
"ap11_spirit_gloating_01" = "Pengembara dari negeri nan jauh datang ke sini untuk memujaku.";
"ap11_spirit_gloating_02" = "Aku adalah orang paling rupawan, berpakaian terbaik, dan paling pintar di pulau ini.";
"ap11_spirit_gloating_03" = "Tapi kamu satu-satunya pria di sini.";
"ap11_spirit_gloating_04" = "Teruslah memuji!";
"ap11_spirit_gloating_05" = "Orang dewasa memang benar-benar sangat aneh.";
"ap11_spirit_scheme_01" = "Apa yang kamu lakukan dengan bintang-bintang ini?";
"ap11_spirit_scheme_02" = "Akulah yang memilikinya. Lima ratus satu juta jumlahnya.";
"ap11_spirit_scheme_03" = "Untuk apa memiliki bintang-bintang?";
"ap11_spirit_scheme_04" = "Itu membuatku jadi kaya.";
"ap11_spirit_scheme_05" = "Kenapa kau ingin jadi kaya?";
"ap11_spirit_scheme_06" = "Untuk membeli lebih banyak bintang.";
"ap11_spirit_scheme_07" = "Orang dewasa benar-benar sangat luar biasa.";
"ap11_spirit_slouch_01" = "Kenapa kamu minum air hitam itu?";
"ap11_spirit_slouch_02" = "Agar aku bisa lupa.";
"ap11_spirit_slouch_03" = "Melupakan apa?";
"ap11_spirit_slouch_04" = "Lupa kalau diriku malu.";
"ap11_spirit_slouch_05" = "Malu akan apa?";
"ap11_spirit_slouch_06" = "Malu meminum air hitam!";
"ap11_spirit_slouch_07" = "Orang dewasa benar-benar amat sangat aneh.";
"ap11_spirit_sneeze_01" = "Halo, kau sedang baca apa? ";
"ap11_spirit_sneeze_02" = "Tolong jangan ganggu aku. Aku sedang mempelajari pengunjung dari planet lain.";
"ap11_spirit_sneeze_03" = "Orang dewasa memang menarik. ";
"ap11_spirit_stretch_01" = "Kau tampak kelelahan! Kenapa kau terus menyalakan lampunya?";
"ap11_spirit_stretch_02" = "Aku diperintahkan untuk menyalakan lampu saat lampunya mati.";
"ap11_spirit_stretch_03" = "Lalu kenapa kau mematikannya setelah menyalakannya?";
"ap11_spirit_stretch_04" = "Aku juga diperintahkan untuk mematikan lampu saat lampunya menyala.";
"ap11_spirit_stretch_05" = "Kalau begitu kau memang tak beruntung.";
"ap11_spirit_stretch_06" = "Perintah adalah perintah.";
"ap12_fetch_01_candle_01" = "Antarkan lilin ini ke <1>Tukang Lonceng Pengutak-atik</1>.";
"ap12_fetch_01_candle_02" = "Antarkan lilin ini ke <1>Tukang Berbakat</1>.";
"ap12_fetch_01_candle_03" = "Antarkan lilin ini ke <1>Navigator Ceria</1>.";
"ap12_fetch_01_candle_04" = "Antarkan lilin ini ke <1>Pembisik Cahaya</1>.";
"ap12_fetch_01_candle_05" = "Antarkan sisa lilin ke roh mereka.";
"ap12_fetch_01_candle_06" = "Roh ini belum siap. Antarkan lilinmu ke roh lainnya terlebih dahulu.";
"ap12_fetch_01_candle_07" = "Roh ini sudah menerima lilinnya.";
"ap12_fetch_01_cine_00" = "<1>Jalur Angin</1> yang menyambungkan semesta terblokir.";
"ap12_fetch_01_cine_01" = "Bantu roh untuk membukanya kembali dengan kemampuan terbangmu.";
"ap12_fetch_01_cine_02" = "Bagikan lilin ke para roh untuk membawa kembali cahaya ke <1>Jalur Angin</1>.";
"ap12_fetch_02_cine_00" = "Perahu yang menghubungkan para roh menghilang di bawah awan.";
"ap12_fetch_02_cine_01" = "Terbang melintasi awan untuk membawa kembali perahu itu.";
"ap12_fetch_02_cine_02" = "Antarkan kepingan yang kamu temukan ke <1>Pemandu Terbang</1>.";
"ap12_fetch_02_meditate" = "Bangun kembali perahu dengan menemukan kepingan yang hilang.";
"ap12_fetch_02_return_01" = "Kembali ke para roh dengan terbang kembali melintasi awan.";
"ap12_fetch_03_meditate" = "Dapatkan kembali lonceng yang dibawa burung-burung!";
"ap12_fetch_04_meditate" = "Lewati <1>Jalur Angin</1> untuk menyelamatkan makhluk-makhluk yang terjebak.";
"ap12_fetch_05_cine_01" = "Temui <1>Pemandu Terbang</1> untuk menyelesaikan pemulihan <1>Jalur Angin</1>.";
"ap12_fetch_05_manta_01" = "Temukan dan bakar semua kegelapan di batu ini sehingga <1>Manta Agung</1> bisa menghancurkannya.";
"ap12_fetch_05_manta_02" = "Naiki Manta Agung kembali ke Pulau.";
"ap12_fetch_05_meditate" = "Selamatkan manta yang terjebak dan hilangkan kegelapan.";
