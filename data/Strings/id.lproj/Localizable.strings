"ap21_quest02_narration_01" = "Pada suatu hari, seekor monster datang ke Oasis Bulan Sabit";
"ap21_quest02_narration_02" = "Semua hewan bersembunyi dalam ketakutan.";
"ap21_quest02_narration_03" = "Rusa Sembilan Warna pergi mencari binatang-binatang itu...";
"ap21_quest02_narration_04" = "... tapi semuanya hilang.";
"ap21_quest02_narration_05" = "Rusa Sembilan Warna tahu apa yang harus dilakukannya.";
"ap21_quest02_narration_06" = "Dengan kebajikan, Rusa Sembilan Warna";
"ap21_quest02_narration_07" = "melindungi semua makhluk hidup.";
"ap21_quest03_hint0" = "Terimalah undangan dari <2>Rusa Sembilan Warna</2> untuk mengunjungi oasis bersama-sama.";
"ap21_quest03_narration_01" = "Suatu hari, Rusa Sembilan Warna menginginkan seorang pendamping";
"ap21_quest03_narration_02" = "Bersama sang anak, sang Rusa merayakan keindahan hidup";
"ap21_quest03_narration_03" = "Seekor manta muncul-diburu oleh penduduk desa!";
"ap21_quest03_narration_04" = "Tapi manta itu terlalu cepat...";
"ap21_quest03_narration_05" = "... dan penduduk desa itu jatuh ke dalam danau.";
"ap21_quest03_narration_06" = "Rusa Sembilan Warna tahu apa yang harus dilakukannya.";
"ap21_quest03_narration_07" = "Mengorbankan beberapa keajaiban...";
"ap21_quest03_narration_08" = "... Rusa menghidupkan kembali penduduk desa.";
"ap21_quest03_narration_09" = "Sebagai ucapan terima kasih, penduduk desa menawarkan diri untuk melayani Rusa itu selamanya.";
"ap21_quest03_narration_10" = "Rusa hanya meminta agar pertemuan mereka dirahasiakan...";
"ap21_quest03_narration_11" = "... tetapi penduduk desa tahu bahwa dia bisa mendapatkan imbalan...";
"ap21_quest03_narration_12" = "... karena sang Putri menginginkan Rusa itu.";
"ap21_quest03_narration_13" = "Maka penduduk desa itu kembali untuk memberitahukan kepada Raja tentang apa yang telah dilihatnya.";
"ap21_quest03_narration_14" = "Raja berangkat untuk berburu Rusa.";
"ap21_quest03_narration_15" = "Dengan cara ini, Rusa Sembilan Warna dikhianati.";
"ap21_quest04_hint1" = "Bantu Rusa menyelamatkan para mahluk yang terperangkap di danau.";
"ap21_quest04_narration_01" = "Simak kisah pengkhianatan terhadap Rusa Sembilan Warna.";
"ap21_quest04_narration_02" = "Atas perintah Pemburu Raja,";
"ap21_quest04_narration_03" = "Penduduk Desa mencemari air.";
"ap21_quest04_narration_04" = "Kemudian, sebuah perangkap dipasang untuk menjerat mahluk mulia itu.";
"ap21_quest04_narration_05" = "Racun dari si Pemburu telah membuat para penduduk desa sakit.";
"ap21_quest04_narration_06" = "Sang Rusa ingin menolong, tetapi ia semakin menjadi lemah.";
"ap21_quest04_narration_07" = "Sang Rusa mengorbankan kekuatan terakhirnya untuk menyelamatkan penduduk desa.";
"ap21_quest04_narration_08" = "Sang Raja sangat senang dengan keberhasilan si Pemburu";
"ap21_quest04_narration_09" = "…tapi penduduk desa dibebani dengan rasa bersalah.";
"ap21_quest04_narration_10" = "Dengan cara ini, Rusa Sembilan Warna ditangkap oleh Raja.";
"ap21_quest05_hint0" = "Bersihkan tanaman kegelapan untuk menaikkan lift.";
"ap21_quest05_hint1" = "Kembalilah ke Rusa.";
"ap21_quest05_narration_01" = "Simak kisah penyelamatan Rusa Sembilan Warna.";
"ap21_quest05_narration_02" = "Kegelapan menyelimuti negeri itu.";
"ap21_quest05_narration_03" = "Dalam keadaan lemah, Rusa tidak dapat melawan kegelapan.";
"ap21_quest05_narration_04" = "Penduduk desa berusaha untuk memperbaiki keadaan.";
"ap21_quest05_narration_05" = "Rusa Sembilan Warna tahu apa yang harus dilakukannya.";
"ap21_quest05_narration_06" = "Karena merasa malu, sang Raja bersumpah untuk melindungi semua mahluk sampai akhir hayatnya.";
"ap21_quest05_narration_07" = "Dengan demikian, kedamaian dan kemakmuran kembali ke Kerajaan itu…";
"ap21_quest05_narration_08" = "…dan Rusa menghilang ke alam liar…";
"ap21_quest05_narration_09" = "…hidup bebas sampai akhir hayatnya.";
