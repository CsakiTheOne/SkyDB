"ap11_fetch_02_carry_02" = "Die Erwachsenen haben vergessen, wie Huckepack funktioniert.";
"ap11_fetch_02_door_01" = "Erkunden ist einfach.";
"ap11_fetch_02_door_02" = "Ein Zuhause zu finden weniger.";
"ap11_fetch_02_end_quote" = "Alle Erwachsenen waren einmal Kinder (aber nur wenige erinnern sich daran).";
"ap11_fetch_02_jelly_01" = "Können die Kreaturen in dieser Höhle singen?";
"ap11_fetch_02_jelly_02" = "Ich glaube, dann würden sie uns singend antworten.";
"ap11_fetch_02_meet_01" = "Ich habe mich verirrt ...";
"ap11_fetch_02_meet_02" = "Weißt du, wie man von hier wegkommt?";
"ap11_fetch_02_outro_01" = "Ich war ein Samen, als er mich fand.";
"ap11_fetch_02_outro_02" = "Für ihn erblühte ich strahlend.";
"ap11_fetch_02_outro_03" = "Jetzt sucht er nach neuen Planeten.";
"ap11_fetch_02_outro_04" = "Ob er noch andere Rosen wie mich findet?";
"ap11_fetch_02_top_01" = "Ich kenne einen Herrn, der noch nie an einer Blume gerochen hat.";
"ap11_fetch_02_top_02" = "Er hat noch niemals einen Stern betrachtet.";
"ap11_fetch_02_top_03" = "Er hat noch niemals jemanden geliebt.";
"ap11_fetch_02_top_04" = "Nie hat er etwas anderes getan, als Zahlen zu addieren.";
"ap11_fetch_02_top_05" = "Und er sagt den ganzen Tag nur eines:";
"ap11_fetch_02_top_06" = "\"Ich bin mit Dingen beschäftigt, die Konsequenzen haben!\"";
"ap11_fetch_02_top_07" = "Und deshalb schwillt er vor Stolz an.";
"ap11_fetch_02_top_08" = "Aber er ist kein Mann.";
"ap11_fetch_02_top_09" = "Er ist ein Pilz!";
"ap11_fetch_02_whale_01" = "Ist hier jemand?";
"ap11_fetch_02_whale_02" = "Was für eine majestätische Kreatur?!";
"ap11_fetch_02_whale_03" = "Kann der hier gezähmt werden?";
"ap11_fetch_03_beach_01" = "Ich habe noch nie auf einem Planeten so viel Wasser gesehen!";
"ap11_fetch_03_beach_02" = "Ich wünschte, ich könnte sehen, was sich hinter dem Horizont befindet.";
"ap11_fetch_03_beach_03" = "Kannst du mich zur Spitze dieser Insel bringen?";
"ap11_fetch_03_beach_04" = "Lass uns zum Gipfel gehen!";
"ap11_fetch_03_belltower_01" = "Was für ein lustiges Haus!";
"ap11_fetch_03_belltower_02" = "Es ist größer als mein ganzer Planet.";
"ap11_fetch_03_belltower_03" = "Es gefällt mir, dass sie im Wind singt.";
"ap11_fetch_03_belltower_04" = "Sieh nur, es ist mit Glocken dekoriert.";
"ap11_fetch_03_belltower_05" = "Genau wie der Himmel Sternen dekoriert ist.";
"ap11_fetch_03_belltower_06" = "Die Glocken sind bestimmt für die Kreaturen.";
"ap11_fetch_03_belltower_07" = "Alle Kinder wissen das.";
"ap11_fetch_03_belltower_08" = "Machen wir uns auf dem Weg zu der Statue.";
"ap11_fetch_03_bye_01" = "Weißt du ...";
"ap11_fetch_03_bye_02" = "was es bedeutet, \"gezähmt\" zu werden?";
"ap11_fetch_03_bye_03" = "Ein Fuchs meinte, es bedeutet, eine Beziehung zu knüpfen.";
"ap11_fetch_03_bye_04" = "Sieh nur!";
"ap11_fetch_03_bye_05" = "Wenn wir eine Beziehung knüpfen, sind wir aufeinander angewiesen.";
"ap11_fetch_03_bye_06" = "Von den Millionen von Kindern ...";
"ap11_fetch_03_bye_07" = "Bist du für mich einzigartig auf der Welt.";
"ap11_fetch_03_bye_08" = "Und ich bin für dich einzigartig auf der Welt.";
"ap11_fetch_03_end_quote" = "Es kann passieren, dass man ein bisschen weinen muss, wenn man sich hat zähmen lassen ...";
"ap11_fetch_03_geyser_01" = "Das ist ein Vulkan!";
"ap11_fetch_03_geyser_02" = "Ich besitze drei Vulkane und ich reinige sie jede Woche.";
"ap11_fetch_03_geyser_03" = "Zwei Vulkane sind aktiv und der andere ist erloschen.";
"ap11_fetch_03_geyser_04" = "Aber man weiß nie.";
"ap11_fetch_03_geyser_06" = "Klingt nach Wasserfällen, oder?";
"ap11_fetch_03_geyser_07" = "Suchen wir sie!";
"ap11_fetch_03_outro_01" = "Mit meinen fantastischen Dornen";
"ap11_fetch_03_outro_02" = "konnte ich es sogar mit Tigern aufnehmen.";
"ap11_fetch_03_outro_03" = "Aber mein Freund wurde unvorsichtig,";
"ap11_fetch_03_outro_04" = "während ich im Wind schwankte.";
"ap11_fetch_03_outro_05" = "Wird er zurückblicken und sich an mich erinnern?";
"ap11_fetch_03_statue_01" = "Was sind das für Kreaturen?";
"ap11_fetch_03_statue_02" = "Kann man sie zähmen?";
"ap11_fetch_03_statue_03" = "Als Freund fürs Leben?";
"ap11_fetch_03_statue_05" = "Was ist das für ein Geräusch?";
"ap11_fetch_03_statue_06" = "Komm mit mir!";
"ap11_fetch_03_tunnel_01" = "Erkunden wir weiter!";
"ap11_fetch_03_tunnel_02" = "Wohin führt das?";
"ap11_fetch_03_tunnel_03" = "Woah!!!";
"ap11_fetch_03_tunnel_04" = "Das hat Spaß gemacht!";
"ap11_fetch_03_waterfall_01" = "Hörst du ...";
"ap11_fetch_03_waterfall_02" = "Der Wasserfall singt ...";
"ap11_fetch_03_waterfall_03" = "Wasser kann auch gut fürs Herz sein ...";
"ap11_fetch_03_waterfall_hint_01" = "Woher kommt das Wasser?";
"ap11_fetch_03_waterfall_hint_02" = "Suchen wir in aller Ruhe nach dem Ursprung.";
"ap11_fetch_04_burn01_01" = "Es gibt gute Samen und schlechte Samen.";
"ap11_fetch_04_burn02_01" = "Wenn man sich nicht um schlechte Samen wie die von Bananenbrotbäumen kümmert, kann das schlimme Folgen haben.";
"ap11_fetch_04_burn03_01" = "Ob Rettich oder Rose, man sollte Pflanzen wachsen lassen, wo sie wünschen.";
"ap11_fetch_04_burn04_01" = "Diese Blumen erinnern mich an meine Rose in der Ferne.";
"ap11_fetch_04_burn05_01" = "Blumen sind empfindlich.";
"ap11_fetch_04_burn05_02" = "Manchmal muss man sie vor dem Wind beschützen.";
"ap11_fetch_04_burn06_01" = "Ich hörte auf meine Rose, ob sie murrte oder prahlte ...";
"ap11_fetch_04_burn06_02" = "... und selbst, wenn sie manchmal nichts sagte.";
"ap11_fetch_04_burn07_01" = "Es ist Zeit, die du für deine Rose geopfert hast ...";
"ap11_fetch_04_burn07_02" = "... die sie so bedeutsam macht.";
"ap11_fetch_04_burn_start_hint" = "Entferne die Dunkelheit, um dich um die Blumen zu kümmern.";
"ap11_fetch_04_bye01_01" = "Diese Rosen sind so schön.";
"ap11_fetch_04_bye01_02" = "Aber sie sind leer.";
"ap11_fetch_04_bye01_03" = "Meine Rose ist einzigartig auf der Welt.";
"ap11_fetch_04_bye01_04" = "Meine Rose ließ meinen Planeten duften ...";
"ap11_fetch_04_bye01_05" = "... aber ich konnte mich nicht darüber freuen.";
"ap11_fetch_04_bye02_01" = "Mein Fuchs hat mir einmal erzählt ...";
"ap11_fetch_04_bye02_02" = "Nur mit dem Herzen sieht man klar.";
"ap11_fetch_04_bye02_03" = "Die wichtigen Dinge sind mit dem Auge nicht zu erkennen.";
"ap11_fetch_04_end_quote" = "Die Sterne sind schön, weil sie an eine Blume erinnern, die man nicht sieht ...";
"ap11_fetch_04_meet_01" = "Da drüben rührt sich etwas.";
"ap11_fetch_04_meet_02" = "Diese Blumen bekommen fast keine Luft mehr.";
"ap11_fetch_04_outro_01" = "Sein Lachen verstummte.";
"ap11_fetch_04_outro_02" = "Meine Eitelkeit in der Niederlage.";
"ap11_fetch_04_outro_03" = "Ohne meinen Freund an meiner Seite";
"ap11_fetch_04_outro_04" = "fühlten sich die Sonnenuntergänge nicht mehr so schön an.";
"ap11_fetch_04_outro_05" = "Ich frage mich, ob er eines Tages Reue empfinden wird?";
"ap11_fetch_05_bye_01" = "Danke ...";
"ap11_fetch_05_bye_02" = "Ich bin froh, dich als Freund zu haben.";
"ap11_fetch_05_end_quote" = "Du wirst immer verantwortlich sein für das, was du zähmst.";
"ap11_fetch_05_meet_01" = "Das habe ich in der Höhle gefunden, die wir besucht haben.";
"ap11_fetch_05_meet_02" = "Wie funktioniert das?";
"ap11_fetch_05_meet_03" = "Möchtest du es für mich ausprobieren?";
"ap11_fetch_05_outro_01" = "(*hust*)";
"ap11_fetch_05_outro_02" = "An manchen Tagen versüßte ich seinen Planeten.";
"ap11_fetch_05_outro_03" = "An manchen Nächten verwelkte ich vor Heuchelei.";
"ap11_fetch_05_outro_04" = "Aber er konnte weder meine Freundschaft";
"ap11_fetch_05_outro_05" = "noch die Freude in meiner Seele erkennen.";
"ap11_fetch_05_outro_06" = "Ob er es jemals erkennen wird ...?";
"ap11_fetch_05_reflect_01" = "Ich hätte sie nach ihren Taten beurteilen sollen ...";
"ap11_fetch_05_reflect_02" = "und nicht nach ihren Worten.";
"ap11_fetch_05_reflect_04" = "Meinst du, ich bin für meine Rose verantwortlich?";
"ap11_fetch_05_sunset_01" = "Ich liebe Sonnenuntergänge.";
"ap11_fetch_05_sunset_02" = "Ich fühle mich wie zu Hause.";
"ap11_fetch_05_sunset_03" = "Einmal habe ich mir den Sonnenuntergang vierundvierzig Mal angesehen!";
"ap11_fetch_05_sunset_04" = "Ich frage mich, ob meine Rose noch den Sonnenuntergang beobachtet? ";
"ap11_fetch_05_sunset_05" = "Ich frage mich, ob sie sicher vom Wind ist?";
"ap11_fetch_05_sunset_07" = "Könntest du ein anderes Musikstück für mich spielen?";
"ap11_fetch_06_campfire_01" = "Ich werde dir ein Geschenk machen ...";
"ap11_fetch_06_campfire_02" = "Genau das wird mein Geschenk sein.";
"ap11_fetch_06_campfire_03" = "Ich kehre heute auch nach Hause zurück ...";
"ap11_fetch_06_campfire_04" = "Auf einem der Sterne werde ich wohnen.";
"ap11_fetch_06_campfire_05" = "Auf einem der Sterne werde ich lachen.";
"ap11_fetch_06_campfire_06" = "Wenn du nachts in den Himmel schaust ...";
"ap11_fetch_06_campfire_07" = "... wird es für dich sein, als lachten alle Sterne.";
"ap11_fetch_06_campfire_08" = "Das ist um einiges weiter ... um einiges schwieriger ...";
"ap11_fetch_06_campfire_09" = "Ich werde viel mehr Angst haben.";
"ap11_fetch_06_campfire_10" = "Es wird so aussehen, als hätte ich Schmerzen.";
"ap11_fetch_06_campfire_11" = "Sieh dir das nicht an.";
"ap11_fetch_06_campfire_12" = "Bei Nacht wirst du in die Sterne blicken.";
"ap11_fetch_06_campfire_13" = "Mein Stern wird einer der Sterne für dich sein.";
"ap11_fetch_06_campfire_14" = "So wirst du alle Sterne gerne anschauen ...";
"ap11_fetch_06_campfire_15" = "Sie alle werden deine Freunde sein.";
"ap11_fetch_06_end_quote" = "Wie komme ich ihm näher, wie kann ich ihm beistehen ... Das Land der Tränen ist voller Geheimnisse.";
"ap11_fetch_06_outro_01" = "(hustet)";
"ap11_fetch_06_outro_02" = "Meine Zeit ist abgelaufen ...";
"ap11_fetch_06_outro_03" = "Ich flehe nach süßer Vergebung.";
"ap11_fetch_06_outro_04" = "Doch er ist nicht zurückgekehrt.";
"ap11_fetch_06_outro_05" = "Wird er jemals zu mir zurückkehren?";
"ap11_fetch_07_end_quote" = "Mein Stern wird einer der Sterne für dich sein. So wirst du alle Sterne gerne anschauen ... Sie alle werden deine Freunde sein.";
"ap11_fetch_07_end_quote_01" = "Mein Stern wird einer der Sterne für dich sein.";
"ap11_fetch_07_end_quote_02" = "So wirst du alle Sterne gerne anschauen.";
"ap11_fetch_07_end_quote_03" = "Sie alle werden deine Freunde sein.";
"ap11_fetch_07_outro_based_01" = "Basierend auf „Der kleine Prinz“";
"ap11_fetch_07_outro_based_02" = "von";
"ap11_fetch_07_outro_based_03" = "Antoine de Saint-Exupéry";
"ap11_fetch_07_outro_featuring_01" = "mit";
"ap11_fetch_07_outro_geographer_01" = "Niesender Geograf";
"ap11_fetch_07_outro_geographer_02" = "Geograf";
"ap11_fetch_07_outro_inspired_01" = "inspiriert von";
"ap11_fetch_07_outro_king_01" = "Winkender Herrscher";
"ap11_fetch_07_outro_king_02" = "König";
"ap11_fetch_07_outro_lamplighter_01" = "Sich streckender Laternenanzünder";
"ap11_fetch_07_outro_lamplighter_02" = "Laternenanzünder";
"ap11_fetch_07_outro_merchant_01" = "Sternensammler";
"ap11_fetch_07_outro_merchant_02" = "Geschäftsmann";
"ap11_fetch_07_outro_narc_01" = "Schadenfroher Narzisst";
"ap11_fetch_07_outro_narc_02" = "Eingebildeter Kerl";
"ap11_fetch_07_outro_prince_02" = "als";
"ap11_fetch_07_outro_slouch_01" = "Buckliger Soldat";
"ap11_fetch_07_outro_slouch_02" = "Rotbäckiger Mann";
"ap11_fetch_07_outro_thank_01" = "Vielen Dank für’s Spielen ...";
"ap11_fetch_07_outro_thank_02" = "Saison des kleinen Prinzen";
"ap11_spirit_beckon_01" = "Du weißt, dass sie sich nicht bewegen können, oder?";
"ap11_spirit_beckon_02" = "Ich habe das Recht, ihren Gehorsam zu verlangen! Meine Befehle sind vernünftig.";
"ap11_spirit_beckon_03" = "Und jetzt bleib, wo du bist.";
"ap11_spirit_beckon_04" = "Erwachsene sind so merkwürdig.";
"ap11_spirit_gloating_01" = "Ein Reisender ist von weit her gekommen, um mich zu bewundern.";
"ap11_spirit_gloating_02" = "Ich bin der schönste, am besten gekleidete, reichste und intelligenteste Mensch auf der Insel.";
"ap11_spirit_gloating_03" = "Aber du bist ganz allein hier.";
"ap11_spirit_gloating_04" = "Bitte verehre mich weiter!";
"ap11_spirit_gloating_05" = "Die Erwachsenen sind wirklich sehr komisch.";
"ap11_spirit_scheme_01" = "Was machst du mit diesen Sternen?";
"ap11_spirit_scheme_02" = "Sie gehören mir. Fünfhunderteine Million von ihnen.";
"ap11_spirit_scheme_03" = "Und warum besitzt du die Sterne?";
"ap11_spirit_scheme_04" = "Weil ich dann reich bin.";
"ap11_spirit_scheme_05" = "Warum willst du reich sein?";
"ap11_spirit_scheme_06" = "Damit ich mir mehr Sterne kaufen kann.";
"ap11_spirit_scheme_07" = "Die Erwachsenen sind wirklich allesamt sehr außergewöhnlich.";
"ap11_spirit_slouch_01" = "Warum trinkst du das dunkle Wasser?";
"ap11_spirit_slouch_02" = "Damit ich vergessen kann.";
"ap11_spirit_slouch_03" = "Was denn?";
"ap11_spirit_slouch_04" = "Dass ich mich schäme.";
"ap11_spirit_slouch_05" = "Wofür schämst du dich denn?";
"ap11_spirit_slouch_06" = "Ich schäme mich dafür, das dunkle Wasser zu trinken!";
"ap11_spirit_slouch_07" = "Die Erwachsenen sind wirklich sehr, sehr komisch.";
"ap11_spirit_sneeze_01" = "Hallo, was liest du denn?";
"ap11_spirit_sneeze_02" = "Stör mich nicht, bitte. Ich studiere einen Besucher von einem anderen Planeten.";
"ap11_spirit_sneeze_03" = "Die Erwachsenen sind schon interessant.";
"ap11_spirit_stretch_01" = "Du siehst erschöpft aus! Warum schaltest du das Licht immer ein?";
"ap11_spirit_stretch_02" = "Ich habe den Befehl erhalten, das Licht einzuschalten, wenn es ausgeht.";
"ap11_spirit_stretch_03" = "Warum schaltest du es aus, nachdem du es eingeschaltet hast?";
"ap11_spirit_stretch_04" = "Ich habe auch den Befehl erhalten, das Licht auszuschalten, wenn es eingeschaltet wird.";
"ap11_spirit_stretch_05" = "Dann hast du Pech.";
"ap11_spirit_stretch_06" = "Ein Befehl ist ein Befehl.";
"ap12_fetch_01_candle_01" = "Bringe diese Kerze zur <1>Glockenspielschmiedin</1>.";
"ap12_fetch_01_candle_02" = "Bringe diese Kerze zum <1>Talentierten Baumeister</1>.";
"ap12_fetch_01_candle_03" = "Bringe diese Kerze zum <1>Lebhaften Navigator</1>.";
"ap12_fetch_01_candle_04" = "Bringe diese Kerze zur <1>Lichtflüsterin</1>.";
"ap12_fetch_01_candle_05" = "Bringe die restlichen Kerzen zu ihren Geistern.";
"ap12_fetch_01_candle_06" = "Dieser Geist ist nicht bereit. Bringe deine Kerzen zuerst zu einem anderen Geist.";
"ap12_fetch_01_candle_07" = "Dieser Geist hat bereits Kerzen bekommen.";
"ap12_fetch_01_cine_00" = "Die <1>Windpfade</1>, welche die Reiche verbinden, sind blockiert.";
"ap12_fetch_01_cine_01" = "Hilf den Geistern mit deiner Flugfähigkeit, sie wieder zu öffnen.";
"ap12_fetch_01_cine_02" = "Verteile Kerzen an die Geister, um das Licht zurück auf <1>die Windpfade</1> zu bringen.";
"ap12_fetch_02_cine_00" = "Das Boot, das die Geister miteinander verbunden hat, ist unter den Wolken verschwunden.";
"ap12_fetch_02_cine_01" = "Fliege durch die Wolken, um das Boot zurückzubringen.";
"ap12_fetch_02_cine_02" = "Bringe die gefunden Teile zum <1>Flughelfer</1>.";
"ap12_fetch_02_meditate" = "Baue das Boot wieder auf, indem du die fehlenden Teile findest.";
"ap12_fetch_02_return_01" = "Fliege durch die Wolken zurück zu den Geistern.";
"ap12_fetch_03_meditate" = "Hol das Glockenspiel zurück, das die Vögel weggetragen haben!";
"ap12_fetch_04_meditate" = "Navigiere durch den <1>Windpfad</1>, um alle gefangenen Kreaturen zu retten";
"ap12_fetch_05_cine_01" = "Sprich mit dem <1>Flughelfer</1>, um die <1>Windpfade</1> fertigzustellen.";
"ap12_fetch_05_manta_01" = "Finde und verbrenne alle Dunkelheit auf diesen Steinen, damit der <1>Riesenrochen</1> sie zerschlagen kann.";
"ap12_fetch_05_manta_02" = "Reite auf dem Riesenrochen zur Insel zurück.";
"ap12_fetch_05_meditate" = "Rette die gefangenen Riesenrochen und vertreibe die Dunkelheit.";
"ap14_fetch_01_confirm" = "<2>AKT I:</2>\n Lass die Bühne wie den <1>Verborgenen Wald</1> aussehen.";
"ap14_fetch_01_effects_01" = "Jetzt kannst du die <2>Bühne</2> und die dazugehörigen <2>Schalter</2> jederzeit verwenden.";
"ap14_fetch_01_impact" = "<hinticon/> Schalter beeinflussen das Theater auf verschiedene Art und Weise.";
"ap14_fetch_01_numerous" = "<hinticon/> Manche Schalter sorgen bei mehrfachem Betätigen für unterschiedliche Effekte.";
"ap14_fetch_01_start" = "Hilf dem <1>Hektischen Bühnenarbeiter</1>, die Bühne wie den <1>Verborgenen Wald</1> aussehen zu lassen.";
"ap14_fetch_02_confirm" = "<2>AKT II:</2> \n Schreibe eine Geschichte für die <1>Insel der Dämmerung</1>.";
"ap14_fetch_02_desc_01" = "Die <1>Vergessliche Geschichtenerzählerin</1> hat ihren Text vergessen.";
"ap14_fetch_02_direct" = "<hinticon/> Beeinflusse die Handlung, indem du eine einzigartige Geschichte auswählst.";
"ap14_fetch_02_effects_01" = "Der <2>Stuhl des Erzählers</2> und ...";
"ap14_fetch_02_effects_02" = "die <2>tägliche Anzeige</2> stehen dir jetzt zur Verfügung.";
"ap14_fetch_02_start" = "Hilf der <1>Vergesslichen Geschichtenerzählerin</1>, eine Geschichte über die <1>Insel der Dämmerung</1> zu schreiben.";
"ap14_fetch_03_confirm" = "<2>AKT III:</2>\n Probe ein Lied für den <1>Tresor des Wissens</1>.";
"ap14_fetch_03_desc_01" = "Spring für den <1>Entspannten Musiker</1> in der Band ein.";
"ap14_fetch_03_desc_02" = "Der <1>Entspannte Musiker</1> ist nirgendwo zu finden.";
"ap14_fetch_03_effects_01" = "Du kannst jetzt jederzeit eines der beiden <2>Instrumente</2> verwenden.";
"ap14_fetch_03_start" = "Hilf der Band des <1>Entspannten Musikers</1>, ein Lied für den <1>Tresor des Wissens</1> einzustudieren.";
"ap14_fetch_04_confirm" = "<2>AKT IV:</2>\n Heitere die Entmutigte Truppe auf.";
"ap14_fetch_04_effects_01" = "Du hast jetzt Zugang zum Hintereingang des <2>Harmoniesaals</2>.";
"ap14_fetch_04_remaining_01" = "Es gibt nur noch <2>einen Geist</2>, der gefunden und aufgemuntert werden muss.";
"ap14_fetch_04_remaining_02" = "Es gibt noch <2>zwei Geister</2>, die gefunden und aufgemuntert werden müssen.";
"ap14_fetch_04_remaining_03" = "Es gibt noch <2>drei Geister</3>, die gefunden und aufgemuntert werden müssen.";
"ap14_fetch_04_return" = "Kehre mit deiner aufgemunterten Truppe zum <1>Darbietungshelfer</1> zurück!";
"ap14_fetch_04_start" = "Die Geister sind unzufrieden mit ihrer Produktion. Muntere sie ein wenig auf!";
"ap14_fetch_05_confirm" = "<2>AKT V:</2>\n Das Finale";
"ap14_fetch_05_desc_01" = "Sie scheinen ihr Selbstvertrauen verloren zu haben ...";
"ap14_fetch_05_desc_02" = "Geh und versuche, sie ein wenig aufzumuntern!";
"ap14_fetch_05_desc_03" = "Die Nacht der letzten Aufführung ist da";
"ap14_fetch_05_help_dancer" = "Die <1>Bescheidene Tänzerin</1> hat Lampenfieber und kann nicht auftreten - unterstütze sie!";
"ap14_fetch_05_improvise_dance" = "Nutze deine <b>Ausdrücke</b>, um eine Nummer mit der <1>Bescheidenen Tänzerin</1> aufzuführen!";
"ap14_fetch_05_improvise_music" = "Improvisiere mit dem <1>Entspannten Musiker</1>, während die <1>Bescheidene Tänzerin</1> ihr Selbstvertrauen zurückgewinnt!";
"ap14_fetch_05_start" = "Hilf der <1>Bescheidenen Tänzerin</1>, die große Show zu beenden!";
